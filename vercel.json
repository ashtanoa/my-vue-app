{
  "redirects": [
    {
      "source": "/:path*",
      "destination": "http://traffic.lab004.xyz/:path*",
      "permanent": false,
      "has": [
        {
          "type": "host",
          "value": "traffic.lab004.xyz"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://39.104.28.230:3000/$1"
    },
    {
      "source": "/login",
      "destination": "http://39.104.28.230:3000/login"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=0"
        }
      ]
    }
  ]
}